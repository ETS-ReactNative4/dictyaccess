{
  "name": "dictyaccess",
  "version": "0.0.5",
  "private": true,
  "engines": {
    "node": "10.4.0"
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "precommit": "lint-staged",
    "cz": "git-cz"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^1.1.0",
    "circos": "https://github.com/dictybase-playground/circosJS/tarball/master",
    "connected-react-router": "^4.4.1",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#v6.6.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#v1.4.0",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#v0.2.0",
    "dicty-components-redux": "github:dictybase-playground/dicty-components-redux#v2.3.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.5.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-svg-pan-zoom": "^2.16.1",
    "recharts": "^1.1.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "ajv": "^6.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flow-bin": "^0.77.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-enzyme": "^6.0.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "react-styleguidist": "^7.1.3",
    "react-test-renderer": "^16.4.2",
    "recast": "^0.13.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
